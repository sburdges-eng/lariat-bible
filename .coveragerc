[run]
source = modules,app
omit =
    */tests/*
    */__init__.py
    */setup.py
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
precision = 2

[html]
directory = htmlcov
